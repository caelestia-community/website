{
    "plugins": "stylelint-scss",
    "extends": ["stylelint-config-recommended", "stylelint-config-tailwindcss"],
    "rules": {
        "at-rule-no-unknown": null,
        "no-descending-specificity": null,
        "scss/at-rule-no-unknown": [
            true,
            {
                "ignoreAtRules": ["theme"]
            }
        ]
    }
}
